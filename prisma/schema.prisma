
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MIGRATE_DATABASE_URL")
}

model Job {
  id       String    @id @unique @default(cuid())
  company  String
  title    String
  link     String
  tags     String
  type     String
  deadline String
  logo     String
}
