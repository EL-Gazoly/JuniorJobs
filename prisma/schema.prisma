
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Job {
  id       String    @id @unique @default(cuid())
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  tags     String
}

