model Job {
  id       String    @id @unique @default(cuid())
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  tags tags[]
}

model tags{
    id String @id
    job String?
    Job Job? @relation(fields: [job],references: [id])
}
